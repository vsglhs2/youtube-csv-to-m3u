{
  "name": "youtube-csv-to-m3u",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "build-yt-search": "mkdir -p public/yt-search && cd yt-search-browserify && node build",
    "prepare-yt-search": "git clone https://github.com/talmobi/yt-search.git yt-search-repo || true && cd yt-search-repo && rm -rf .git && npm i",
    "postinstall": "npm run prepare-yt-search && npm run build-yt-search",
    "lint": "eslint . --ignore-pattern \"yt-search-repo/*\" --ignore-pattern \"yt-search-browserify/*\" --ignore-pattern \"dist/*\" --ignore-pattern \"src/yt-search/*\"",
    "lint:fix": "eslint --fix . --ignore-pattern \"yt-search-repo/*\" --ignore-pattern \"yt-search-browserify/*\" --ignore-pattern \"dist/*\" --ignore-pattern \"src/yt-search/*\""
  },
  "devDependencies": {
    "@babel/core": "~7.27.1",
    "@babel/preset-env": "~7.27.1",
    "@eslint/js": "^9.27.0",
    "@stylistic/eslint-plugin": "^4.4.0",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@types/yt-search": "^2.10.3",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "@upleveled/babel-plugin-remove-node-prefix": "^1.0.5",
    "babelify": "~10.0.0",
    "browserify": "~17.0.0",
    "diagnostic-channel": "^1.1.1",
    "eslint": "^9.27.0",
    "eslint-plugin-import": "^2.31.0",
    "globals": "^16.2.0",
    "http-browserify": "^1.7.0",
    "jsonpath-plus": "~10.3.0",
    "querystring-es3": "^1.0.0-0",
    "tw-animate-css": "^1.3.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.33.0",
    "util": "^0.12.5",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.14",
